#include<stdio.h>
void main()
{
    int i,n,num=0,f=1;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        num=a[i];
        f=1;
        
        while(num>0)
        {
            f=f*num;
            num--;
        }
        printf("the factorial of %d is %d",a[i],f);
    }
    
    
}
